   "source": [
    "import numpy as np \n",
    "import matplotlib.pyplot as plt \n",
    "x = np.arange(0,360)\n",
    "y = np.sin(x*np.pi/180)\n",
    "z = np.cos(x*np.pi/180)\n",
    "plt.plot(x, y, color=\"blue\")\n",
    "plt.plot(x, z, color=\"red\")\n",
    "plt.xlim(0,360)\n",
    "plt.ylim(-1.2, 1.2)\n",
    "plt.title(\"SIN & COS Function\")\n",
    "plt.show()\n"
   ]
  }
